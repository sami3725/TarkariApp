
@{
    ViewBag.Title = "Login";
}

@using (Html.BeginForm("login","adminusers"))
    {
    <div class="container">
        <div class="card col-md-4 col-sm-4 col-ld-4">
            <div class="card-body">
                <input class="form-control " placeholder="UserName" name="UserName" />
                <input class="form-control mt-4" placeholder="Password" name="Password" />

            </div>
            <div class="card-footer">
                <input class="btn btn-sm btn-outline-info" value="Log in" type="submit" />
            </div>

        </div>
    </div>
    }

